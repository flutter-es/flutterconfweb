{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/main.dart.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/icons/icon-192.png",
        "destination": "/icons/icon-192.png"
      },
      {
        "source": "/icons/icon-512.png",
        "destination": "/icons/icon-512.png"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}